{
  "name": "soda3-query",
  "version": "0.1.0",
  "description": "SODA (Socrata) query client for NodeJS. Compatible with SODA 3",
  "author": "Graham Place <grahamplace.work@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grahamplace/soda3-query.git"
  },
  "keywords": [
    "soda",
    "soda3",
    "socrata",
    "query",
    "client",
    "nodejs"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/grahamplace/soda3-query/issues"
  },
  "homepage": "https://github.com/grahamplace/soda3-query#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier . --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm test"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.13"
  }
}
